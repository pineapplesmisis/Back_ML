<?xml version="1.0" encoding="utf-8" ?>
<rules>
  <ListProductsUrl>
  <item><![CDATA[https://cityblank.ru/]]></item>
  <item><![CDATA[\/catalog\/(\w+)\/]]></item>
  <item><![CDATA[\/catalog\/(\w+)\/\?PAGEN_(\w+)=(\d+)]]></item>
  </ListProductsUrl>
  <ProductUrl>
   <item><![CDATA[\/(catalog)\/([\w\d]+)\/([\w\d_]+)\/]]></item>
  </ProductUrl>
  <UrlBase>
  <![CDATA[https://cityblank.ru]]>
  </UrlBase>
  <ProductName>
  <![CDATA[//h1[@itemprop='name']]]>
  </ProductName>
 <ProductPrice>
    <![CDATA[//div[@class='product-price']]]>
  </ProductPrice>
  <ProductImage takenAttrubute="src">
    <![CDATA[//div[contains(@class,'product-image')]/a/img]]>
  </ProductImage>
</rules>
